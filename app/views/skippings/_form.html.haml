= form_for @skipping do |f|
  -if @skipping.errors.any?
    #error_explanation
      %h2= "#{pluralize(@skipping.errors.count, "error")} prohibited this skipping from being saved:"
      %ul
        - @skipping.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :aspirant_id
    = f.number_field :aspirant_id
  .field
    = f.label :start
    = f.date_select :start
  .field
    = f.label :finish
    = f.date_select :finish
  .actions
    = f.submit 'Save'
