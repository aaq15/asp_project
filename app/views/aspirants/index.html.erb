<h1>Listing aspirants</h1>

<table>
  <tr>
    <th>Aspirant type</th>
    <th>Start study date</th>
    <th>Maybe finish date</th>
    <th>Finish study date</th>
    <th>Scientman</th>
    <th>Study form</th>
    <th>Protect status</th>
    <th>Protect data</th>
    <th>Payment</th>
    <th>Specialiti</th>
    <th>Skip</th>
    <th>Rank</th>
    <th>Start date</th>
    <th>Finish date</th>
    <th>Person</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @aspirants.each do |aspirant| %>
  <tr>
    <td><%= aspirant.aspirant_type %></td>
    <td><%= aspirant.start_study_date %></td>
    <td><%= aspirant.maybe_finish_date %></td>
    <td><%= aspirant.finish_study_date %></td>
    <td><%= aspirant.scientman_id %></td>
    <td><%= aspirant.study_form %></td>
    <td><%= aspirant.protect_status %></td>
    <td><%= aspirant.protect_data %></td>
    <td><%= aspirant.payment %></td>
    <td><%= aspirant.specialiti_id %></td>
    <td><%= aspirant.skip %></td>
    <td><%= aspirant.rank %></td>
    <td><%= aspirant.start_date %></td>
    <td><%= aspirant.finish_date %></td>
    <td><%= aspirant.person_id %></td>
    <td><%= link_to 'Show', aspirant %></td>
    <td><%= link_to 'Edit', edit_aspirant_path(aspirant) %></td>
    <td><%= link_to 'Destroy', aspirant, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

